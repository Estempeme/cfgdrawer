<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
	<extension
         point="org.eclipse.ui.actionSets">
      <actionSet
            label="Sample Action Set"
            visible="true"
            id="Semesterprojekt_Uebung">
         <menu
               label="Bugminer"
               id="sampleMenu">
            <separator
                  name="sampleGroup">
            </separator>
         </menu>
         <action
               label="&amp;Draw Graph"
               icon="icons/bugsmall.gif"
               class="Graph.Plugin"
               tooltip="Click here to open the GUI for the Bugminer CFG Drawer."
               menubarPath="sampleMenu/sampleGroup"
               toolbarPath="sampleGroup"
               id="Graph.Plugin">
         </action>
      </actionSet>
   </extension>

</plugin>
